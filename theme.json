{
	"settings": {
		"appearanceTools": true,
		"layout": {
			"contentSize": "620px",
			"wideSize": "1000px"
		},
		"spacing": {
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif",
					"name": "System Font",
					"slug": "system-font"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Jua",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/jua_normal_400.ttf"
							]
						}
					],
					"fontFamily": "Jua",
					"slug": "jua"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Gaegu",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/gaegu_normal_700.ttf"
							]
						},
						{
							"fontFamily": "Gaegu",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/gaegu_normal_400.ttf"
							]
						},
						{
							"fontFamily": "Gaegu",
							"fontStyle": "normal",
							"fontWeight": "300",
							"src": [
								"file:./assets/fonts/gaegu_normal_300.ttf"
							]
						}
					],
					"fontFamily": "Gaegu",
					"slug": "gaegu"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Just Me Again Down Here",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/just-me-again-down-here_normal_400.ttf"
							]
						}
					],
					"fontFamily": "Just Me Again Down Here",
					"slug": "just-me-again-down-here"
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"blocks": {
			"core/navigation": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--jua)",
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			}
		},
		"elements": {
			"button": {
				"color": {
					"background": "#ff00ff",
					"text": "var(--wp--preset--color--white)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--just-me-again-down-here)"
				}
			},
			"caption": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--gaegu)",
					"fontStyle": "normal",
					"fontWeight": "600"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "clamp(3.05rem, 3.54vw + 2.17rem, 5rem)",
					"lineHeight": "1.4"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "clamp(2.44rem, 2.38vw + 1.85rem, 3.75rem)",
					"lineHeight": "1.4"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "clamp(1.95rem, 1.56vw + 1.56rem, 2.81rem)",
					"lineHeight": "1.4"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "clamp(1.56rem, 1vw + 1.31rem, 2.11rem)",
					"lineHeight": "1.4"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "clamp(1.25rem, 0.61vw + 1.1rem, 1.58rem)",
					"lineHeight": "1.4"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "clamp(1rem, 0.34vw + 0.91rem, 1.19rem)",
					"lineHeight": "1.4"
				}
			},
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--jua)"
				}
			},
			"link": {
				"typography": {
					"textDecoration": "none"
				}
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--gaegu)",
			"fontSize": "16px",
			"fontStyle": "normal",
			"fontWeight": "500",
			"lineHeight": "1.7"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header"
		},
		{
			"area": "footer",
			"name": "footer"
		}
	],
	"version": 2,
	"$schema": "https://schemas.wp.org/wp/6.3/theme.json"
}